syntax C:
["] (5) string
[0] (7) zero
[#] (1) preproc
. (0)

string:
	[\\] (6) string_escape
	["] (5) !pop
	. (5)

string_escape:
	[abefnrv\\"'\n] (6) string
	[x][a-fA-F0-9]2 (6) string
	[u][a-fA-F0-9]4 (6) string
	[U][a-fA-F0-9]8 (6) string
	. (REVERSE 2) string

preproc:
	[\t ] (1)
	[d][e][f][i][n][e] (1) 




