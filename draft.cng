bind .lazy:

HOME >home
END >end
LEFT <N
RIGHT >N
UP `N
DOWN .N

bind .normal_movement:

0 >home
$ >end
g g .home .N & `1
g G, G .end `N & .1
BACK $-N
SPACE $N
^J .1 & >home
h <N
l >N
k `N
j .N

bind normal: lazy, normal_movement

^W h <wN
^W j .wN
^W k `wN
^W l >wN
^W SPACE $wN
^W BACK $w-N
^W n !new
^W w !close
^W v !vsplit
^W s !hsplit

bind $insert: lazy

include @edit "edit.cng"
include @fileviewer "fileviewer.cng"

