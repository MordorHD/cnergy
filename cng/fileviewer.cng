# All these keybinds are available only inside of
# the fileview window type
bind fileviewer::normal:

<enter>		call choose
<tab>		call insert
sa		call sort_alphabetical
ss		call sort_changetime
sm		call sort_modificationtime
st		call toggle_sort_type
sr		call toggle_sort_reverse
h		call toggle_hidden

bind $fileviewer::insert:

<enter>		call choose
<tab>		call normal
<back>		<x1
<del>		>x1
<esc>		call normal
